{
  "name": "Quantity Literals",
  "description": "Tests for quantity literal parsing and evaluation",
  "tests": [
    {
      "name": "quantity literal - simple unit",
      "expression": "5 'mg'",
      "input": [],
      "expected": [
        {
          "value": 5,
          "unit": "mg"
        }
      ],
      "tags": ["quantity", "literal", "ucum"]
    },
    {
      "name": "quantity literal - decimal value",
      "expression": "3.14 'km'",
      "input": [],
      "expected": [
        {
          "value": 3.14,
          "unit": "km"
        }
      ],
      "tags": ["quantity", "literal", "ucum"]
    },
    {
      "name": "quantity literal - UCUM unit with brackets",
      "expression": "98.6 '[degF]'",
      "input": [],
      "expected": [
        {
          "value": 98.6,
          "unit": "[degF]"
        }
      ],
      "tags": ["quantity", "literal", "ucum"]
    },
    {
