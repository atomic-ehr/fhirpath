  "devDependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@rgrove/parse-xml": "^4.2.0",
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "@types/node": "22.13.14",
    "@types/turndown": "^5.0.5",
    "jsdom": "^26.1.0",
    "tsup": "8.5.0",
    "turndown": "^7.2.0",
