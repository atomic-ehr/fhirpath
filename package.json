{
  "name": "@atomic-ehr/fhirpath",
  "version": "0.0.1",
  "description": "A TypeScript implementation of FHIRPath",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
